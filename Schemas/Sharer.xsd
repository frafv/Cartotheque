									<xs:documentation xml:lang="en">Last Sunday of March at 2:00 – Last Sunday of October at 3:00</xs:documentation>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="UTC+8 AWST">
							<xs:annotation>
								<xs:documentation xml:lang="en">Australian Western Standard Time</xs:documentation>
								<xs:appinfo source="DST">
									<xs:documentation xml:lang="en">Last Sunday of October at 2:00 – Last Sunday of March at 3:00</xs:documentation>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="UTC+8:45 CWT">
							<xs:annotation>
								<xs:documentation xml:lang="en">Central West Time (Australia)</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="UTC+9">
							<xs:annotation>
								<xs:documentation xml:lang="en">Japan Standard Time, Korea Standard Time</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="UTC+9 USZ8">
							<xs:annotation>
								<xs:documentation xml:lang="en">Yakutsk Standard Time, Amur Time(Russia)</xs:documentation>
								<xs:appinfo source="DST">
									<xs:documentation xml:lang="en">Last Sunday of March at 2:00 – Last Sunday of October at 3:00</xs:documentation>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="UTC+9:30">
							<xs:annotation>
								<xs:documentation xml:lang="en">Australian Central Standard Time (Australia Northern Territory)</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="UTC+9:30 ACST">
							<xs:annotation>
								<xs:documentation xml:lang="en">Australian Central Standard Time</xs:documentation>
								<xs:appinfo source="DST">
									<xs:documentation xml:lang="en">First Sunday of October at 2:00 – First Sunday of April at 3:00</xs:documentation>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="UTC+10 AEST">
							<xs:annotation>
								<xs:documentation xml:lang="en">Australian Eastern Standard Time</xs:documentation>
								<xs:appinfo source="DST">
									<xs:documentation xml:lang="en">First Sunday of October at 2:00 – First Sunday of April at 3:00</xs:documentation>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="UTC+10">
							<xs:annotation>
								<xs:documentation xml:lang="en">Australian Eastern Standard Time (Australian Queensland), West Pacific Standard Time</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="UTC+10 Tasmania">
							<xs:annotation>
								<xs:documentation xml:lang="en">Tasmania Standard Time</xs:documentation>
								<xs:appinfo source="DST">
									<xs:documentation xml:lang="en">First Sunday of October at 2:00 – First Sunday of April at 3:00</xs:documentation>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="UTC+10 USZ9">
							<xs:annotation>
								<xs:documentation xml:lang="en">Vladivostok Time (Russia)</xs:documentation>
								<xs:appinfo source="DST">
									<xs:documentation xml:lang="en">Last Sunday of March at 2:00 – Last Sunday of October at 3:00</xs:documentation>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="UTC+11">
							<xs:annotation>
								<xs:documentation xml:lang="en">Central Pacific Standard Time, Okhotsk Time (Russia)</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="UTC+11:30 NFT">
							<xs:annotation>
								<xs:documentation xml:lang="en">Norfolk Time (Australia)</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="UTC+12">
							<xs:annotation>
								<xs:documentation xml:lang="en">Fiji Standard Time, Kamchatka Time (Russia)</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="UTC+12 NZST">
							<xs:annotation>
								<xs:documentation xml:lang="en">New Zealand Standard Time</xs:documentation>
								<xs:appinfo source="DST">
									<xs:documentation xml:lang="en">Last Sunday of September at 2:00 – First Sunday of April at 3:00</xs:documentation>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="UTC+13 TOT">
							<xs:annotation>
								<xs:documentation xml:lang="en">Tonga Standard Time, Phoenix Islands Time</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="p:Period"/>
			<xs:attributeGroup ref="p:Prof"/>
			<xs:attribute name="Company" type="xs:IDREF"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Address">
		<xs:annotation>
			<xs:documentation xml:lang="ru">Адрес</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute ref="xml:lang" use="required"/>
					<xs:attribute name="Country" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Страна</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="PostIndex">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Почтовый индекс</xs:documentation>
							<xs:documentation xml:lang="en">Post Index</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:int">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="999999"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="Region" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Район</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="City" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Город</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Street" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Улица</xs:documentation>
							<xs:documentation xml:lang="en">Street</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="House">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Дом</xs:documentation>
							<xs:documentation xml:lang="en"/>
							<xs:appinfo source="Label">
								<xs:documentation xml:lang="ru">Дом</xs:documentation>
								<xs:documentation xml:lang="ru" source="Abbr">д.</xs:documentation>
								<xs:documentation xml:lang="en"/>
							</xs:appinfo>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="\d+.?(/\d+.?)?"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="Frame" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Корпус</xs:documentation>
							<xs:documentation xml:lang="en">Frame</xs:documentation>
							<xs:appinfo source="Label">
								<xs:documentation xml:lang="ru">Корпус</xs:documentation>
								<xs:documentation xml:lang="ru" source="Abbr">к.</xs:documentation>
								<xs:documentation xml:lang="en">Frame</xs:documentation>
								<xs:documentation xml:lang="en" source="Abbr">frame</xs:documentation>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Porch" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Подъезд</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="PorchCode" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Домофон</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Floor" type="xs:unsignedInt">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Этаж</xs:documentation>
							<xs:documentation xml:lang="en">Floor</xs:documentation>
							<xs:appinfo source="Label">
								<xs:documentation xml:lang="ru">Этаж</xs:documentation>
								<xs:documentation xml:lang="ru" source="Abbr">эт.</xs:documentation>
								<xs:documentation xml:lang="en">Floor</xs:documentation>
								<xs:documentation xml:lang="en" source="Abbr">floor</xs:documentation>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Apartment" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Квартира</xs:documentation>
							<xs:documentation xml:lang="en"/>
							<xs:appinfo source="Label">
								<xs:documentation xml:lang="ru">Квартира</xs:documentation>
								<xs:documentation xml:lang="ru" source="Abbr">кв.</xs:documentation>
								<xs:documentation xml:lang="en"/>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Metro" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Ближайшее метро</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Phone">
		<xs:annotation>
			<xs:documentation xml:lang="ru">Телефон</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="Type" default="Phone">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Тип телефона</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:list>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Phone">
											<xs:annotation>
												<xs:documentation xml:lang="ru">Телефон</xs:documentation>
												<xs:documentation xml:lang="en">Phone</xs:documentation>
												<xs:appinfo source="Label">
													<xs:documentation xml:lang="ru">Телефон</xs:documentation>
													<xs:documentation xml:lang="ru" source="Abbr">тел.</xs:documentation>
													<xs:documentation xml:lang="en">Phone</xs:documentation>
												</xs:appinfo>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="PhoneFax">
											<xs:annotation>
												<xs:documentation xml:lang="ru">Телефон/Факс</xs:documentation>
												<xs:documentation xml:lang="en">Phone/Fax</xs:documentation>
												<xs:appinfo source="Label">
													<xs:documentation xml:lang="ru">Телефон/Факс</xs:documentation>
													<xs:documentation xml:lang="ru" source="Abbr">тел./факс</xs:documentation>
													<xs:documentation xml:lang="en">Phone/Fax</xs:documentation>
												</xs:appinfo>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Fax">
											<xs:annotation>
												<xs:documentation xml:lang="ru">Факс</xs:documentation>
												<xs:documentation xml:lang="en">Fax</xs:documentation>
												<xs:appinfo source="Label">
													<xs:documentation xml:lang="ru">Факс</xs:documentation>
													<xs:documentation xml:lang="ru" source="Abbr">факс</xs:documentation>
													<xs:documentation xml:lang="en">Fax</xs:documentation>
												</xs:appinfo>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Modem">
											<xs:annotation>
												<xs:documentation xml:lang="ru">Модем</xs:documentation>
												<xs:documentation xml:lang="en">Modem</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Mobile">
											<xs:annotation>
												<xs:documentation xml:lang="ru">Сотовый</xs:documentation>
												<xs:documentation xml:lang="en">Mobile</xs:documentation>
												<xs:appinfo source="Label">
													<xs:documentation xml:lang="ru">Сотовый</xs:documentation>
													<xs:documentation xml:lang="ru" source="Abbr">моб.</xs:documentation>
													<xs:documentation xml:lang="en">Mobile</xs:documentation>
												</xs:appinfo>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Private">
											<xs:annotation>
												<xs:documentation xml:lang="ru">Личный</xs:documentation>
												<xs:documentation xml:lang="en">Private</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
									</xs:restriction>
								</xs:simpleType>
							</xs:list>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="CountryCode">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Код страны</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="\d+"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="Code">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Код города</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="\d+"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="Number">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Номер телефона</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="\d+"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="Ext">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Внутренний номер</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="\d+"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="Operator" type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="ru">Оператор связи</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attributeGroup ref="p:Period"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="EMail">
		<xs:annotation>
			<xs:documentation xml:lang="ru">Электронная почта</xs:documentation>
			<xs:documentation xml:lang="en">e-mail</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:anyURI">
					<xs:attribute name="URL" type="xs:anyURI"/>
					<xs:attributeGroup ref="p:Period"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Web">
		<xs:annotation>
			<xs:documentation xml:lang="ru">Домашняя страница в интернете</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="URL" type="xs:anyURI" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Photo">
		<xs:annotation>
			<xs:documentation xml:lang="ru">Фотография</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="URL" type="xs:anyURI" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Messenger">
		<xs:annotation>
			<xs:documentation xml:lang="ru">Аккаунты на серверах мгновенных сообщений</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="Type" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="ICQ"/>
								<xs:enumeration value="Skype"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="Number">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="\d+"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="Nickname" type="xs:string"/>
					<xs:attributeGroup ref="p:Period"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Document">
		<xs:annotation>
			<xs:documentation xml:lang="ru">Официальный документ удостоверяющий личность</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="p:Agency" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="Type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="DrivingLicence"/>
						<xs:enumeration value="ForeignPassport"/>
						<xs:enumeration value="Passport"/>
						<xs:enumeration value="VISA"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Series" type="xs:string"/>
			<xs:attribute name="Number" type="xs:string"/>
			<xs:attribute name="Country">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="RU"/>
						<xs:enumeration value="UA"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="p:Period"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Agency">
		<xs:annotation>
			<xs:documentation xml:lang="ru">Организация выдавшая документ</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute ref="xml:lang" use="required"/>
					<xs:attribute name="Code" type="xs:string"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
</xs:schema>
