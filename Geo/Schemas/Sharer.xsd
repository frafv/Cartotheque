							<xs:documentation xml:lang="en">SA Pacific Standard Time, US Eastern Standard Time</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="UTC-5 EST">
						<xs:annotation>
							<xs:documentation xml:lang="en">Eastern Standard Time</xs:documentation>
							<xs:appinfo source="DST">
								<xs:documentation xml:lang="en">First Sunday of April at 2:00 – Last Sunday of October at 2:00</xs:documentation>
							</xs:appinfo>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="UTC-4:30 VET">
						<xs:annotation>
							<xs:documentation xml:lang="en">Venezuela Standard Time</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="UTC-4">
						<xs:annotation>
							<xs:documentation xml:lang="en">SA Western Standard Time</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="UTC-4 AST">
						<xs:annotation>
							<xs:documentation xml:lang="en">Atlantic Standard Time</xs:documentation>
							<xs:appinfo source="DST">
								<xs:documentation xml:lang="en">First Sunday of April at 2:00 – Last Sunday of October at 2:00</xs:documentation>
							</xs:appinfo>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="UTC-4 Manaus">
						<xs:annotation>
							<xs:documentation xml:lang="en">Central Brazilian Standard Time</xs:documentation>
							<xs:appinfo source="DST">
								<xs:documentation xml:lang="en">Third Saturday of October at 23:59:59 – Second Saturday of February at 23:59:59</xs:documentation>
							</xs:appinfo>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="UTC-4 PST">
