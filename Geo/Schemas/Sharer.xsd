							<xs:documentation xml:lang="en">Pacific SA Standard Time</xs:documentation>
							<xs:appinfo source="DST">
								<xs:documentation xml:lang="en">Second Saturday of October at 23:59:59 – Last Saturday of March at 23:59:59</xs:documentation>
							</xs:appinfo>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="UTC-4 PYT">
						<xs:annotation>
							<xs:documentation xml:lang="en">Paraguay Standard Time</xs:documentation>
							<xs:appinfo source="DST">
								<xs:documentation xml:lang="en">Third Saturday of October at 23:59:59 – Second Saturday of March at 23:59:59</xs:documentation>
							</xs:appinfo>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="UTC-3:30 NST">
						<xs:annotation>
							<xs:documentation xml:lang="en">Newfoundland Standard Time</xs:documentation>
							<xs:appinfo source="DST">
								<xs:documentation xml:lang="en">Second Sunday of March at 0:01 – First Sunday of November at 0:01</xs:documentation>
							</xs:appinfo>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="UTC-3">
						<xs:annotation>
							<xs:documentation xml:lang="en">SA Eastern Standard Time</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="UTC-3 ART">
						<xs:annotation>
							<xs:documentation xml:lang="en">Argentina Standard Time</xs:documentation>
							<xs:appinfo source="DST">
								<xs:documentation xml:lang="en">First Thursday of January at 0:00 – Second Saturday of March at 23:59:59</xs:documentation>
							</xs:appinfo>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="UTC-3 BRT">
						<xs:annotation>
							<xs:documentation xml:lang="en">E. South America Standard Time</xs:documentation>
							<xs:appinfo source="DST">
								<xs:documentation xml:lang="en">Third Saturday of October at 23:59:59 – Second Saturday of February at 23:59:59</xs:documentation>
							</xs:appinfo>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="UTC-3 GST">
						<xs:annotation>
							<xs:documentation xml:lang="en">Greenland Standard Time</xs:documentation>
